{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"preserveConstEnums": true,
		"strict": true,
		"target": "esnext",
		"module": "esnext",
		"rootDir": "./",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"downlevelIteration": true
	}
}
